; 4BYTE MULTIPLICATION 
HIGH1 EQU 31H
LOW1 EQU 30H

HIGH2 EQU 33H
LOW2 EQU 32H


MOV 30H, #11H 
MOV 31H, #11H 

MOV 32H, #11H
MOV 33H, #11H 


 ; RESULT 43, 42, 41, 40 
MOV A, LOW1 
MOV B, LOW2 

MUL AB 

MOV 40H, A 
MOV 41H, B 

MOV A, LOW2 
MOV B, HIGH1 

MUL AB 

ADD A, 41H 
MOV 41H, A 

MOV A,B 
ADDC A, 42H 
MOV 42H, A 

MOV A, HIGH1
MOV B,LOW1 

MUL AB 

ADD A, 41H 
MOV 41H, A 

MOV A,B 
ADDC A, 42H 
MOV 42H, A 

MOV A, HIGH1 
MOV B, HIGH2 

MUL AB 

ADD A, 42H 
MOV 42H, A

MOV A,B 
ADDC A, 43H 
MOV 43H, A 




SJMP $ 
